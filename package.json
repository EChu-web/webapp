  "scripts": {
    "dev": "npm-run-all --parallel dev-client dev-server",
    "dev-client": "npm-run-all update-schema dev-client-webpack",
    "dev-client-webpack": "node --harmony -- \"webpack/development server.entry.js\"",
    "dev-server-api": "nodemon ./code/api/entry.js",
    "dev-server-rendering": "nodemon ./code/server/entry.js",
    "dev-server": "npm-run-all --parallel dev-server-api dev-server-rendering",
    "production": "npm-run-all production-build-client production-server",
    "production-build-client": "npm-run-all update-schema production-build-client-webpack",
    "production-build-client-webpack": "webpack --verbose --colors --display-error-details --config \"webpack/production build.entry.js\"",
    "production-server-api": "node --harmony ./code/api/entry.js --production",
    "production-server-rendering": "node --harmony ./code/server/entry --production",
    "production-server": "npm-run-all --parallel production-server-api production-server-rendering",
    "update-schema": "babel-node \"./automation/relay/update schema.js\"",
    "_postinstall": "bower install",
    "_lint": "eslint -c .eslintrc src",
    "_dev-webpack": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build",
    "_deploy": "NODE_ENV=production webpack -p --config webpack.config.js",
    "_test": "mocha --compilers js:babel/register --colors --bail --reporter spec test/",
    "_test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel/register --colors --reporter dot test/",
    "_test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --compilers js:babel/register --colors --reporter spec test/",
    "_gh-pages": "webpack --action=gh-pages --colors --config ./config",
    "_deploy-gh-pages": "TARGET=gh-pages node ./config/deploy-gh-pages.js"
  },
