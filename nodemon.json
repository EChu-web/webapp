{
	"script": "server/main.js",
	"watch": ["server/**/*.js"],
	"watch_interval": 300,
	"dirs": ["server"],
	"execMap": {
		"js": "babel-node --stage 0",
		"js_old": "node --harmony --use_strict"
	},
	"ignore": [
		".git",
		"node_modules/**/*",
		"build/**/*"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"legacyWatch": false,
	"verbose": true,
	"dump": false
}