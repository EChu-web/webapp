{
	"script": "code/server/entry.js",
	"watch": ["code/**/*.js"],
	"watch_interval": 300,
	"ignore":
	[
		".git",
		"node_modules/**/*",
		"build/**/*"
	],
	"env":
	{
		"NODE_ENV": "development"
	},
	"execMap":
	{
		"js": "node --harmony"
	},
	"legacyWatch": false,
	"verbose": false,
	"dump": false
}